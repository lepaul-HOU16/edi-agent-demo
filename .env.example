# CDK API Configuration
# API Gateway URL for the CDK-deployed backend
# This is the base URL for all REST API calls
VITE_API_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com

# EDI/OSDU API Credentials
EDI_CLIENT_ID=your_client_id_here
EDI_CLIENT_SECRET=your_client_secret_here

# AWS Credentials (if needed locally)
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_SESSION_TOKEN=your_session_token_here

# LangChain API Key (if using)
# LANGCHAIN_API_KEY=your_langchain_key_here

# ============================================
# Renewable Energy Integration Configuration
# ============================================

# Enable/disable renewable energy features
# Set to 'true' to enable renewable energy agent routing
NEXT_PUBLIC_RENEWABLE_ENABLED=false

# AWS Bedrock AgentCore endpoint URL
# This is the invoke endpoint for the deployed renewable energy multi-agent system
# Example: https://agentcore.us-west-2.amazonaws.com/invoke/renewable-wind-farm
# Obtain this URL after deploying the renewable backend using deploy-to-agentcore.sh
NEXT_PUBLIC_RENEWABLE_AGENTCORE_ENDPOINT=

# S3 bucket for renewable energy artifacts
# This bucket stores generated maps, layouts, simulation results, and reports
# Should match the bucket configured in SSM parameter: /wind-farm-assistant/s3-bucket-name
NEXT_PUBLIC_RENEWABLE_S3_BUCKET=

# AWS Region for renewable energy services
# Should match the region where AgentCore and S3 bucket are deployed
# Default: us-west-2
NEXT_PUBLIC_RENEWABLE_AWS_REGION=us-west-2

# ============================================
# EDIcraft Agent Configuration
# ============================================

# AWS Bedrock AgentCore Configuration
# Agent ID for the deployed EDIcraft Bedrock AgentCore agent
# Obtain this after deploying the agent using edicraft-agent/DEPLOYMENT_GUIDE.md
BEDROCK_AGENT_ID=your_agent_id_here

# Agent Alias ID for the deployed EDIcraft Bedrock AgentCore agent
# Typically 'TSTALIASID' for test alias or a custom alias ID
BEDROCK_AGENT_ALIAS_ID=TSTALIASID

# AWS Region where the Bedrock AgentCore agent is deployed
# Default: us-east-1
BEDROCK_REGION=us-east-1

# Minecraft Server Connection
# Host address for the Minecraft server running EDIcraft visualization
MINECRAFT_HOST=edicraft.nigelgardiner.com

# RCON port for Minecraft server remote console access
# Port 49000 is for game connections, 49001 is for RCON
MINECRAFT_RCON_PORT=49001

# RCON password for authenticating with Minecraft server
# This should be kept secure and match the password configured in the Minecraft server
MINECRAFT_RCON_PASSWORD=your_rcon_password_here

# OSDU Platform Credentials for EDIcraft
# These credentials are used to access subsurface data from the OSDU platform
EDI_USERNAME=your_edi_username
EDI_PASSWORD=your_edi_password
EDI_CLIENT_ID=your_edi_client_id
EDI_CLIENT_SECRET=your_edi_client_secret
EDI_PARTITION=your_partition_name
EDI_PLATFORM_URL=https://your-osdu-platform-url.com

# MCP Server Configuration
# URL for the Model Context Protocol server (if using external MCP server)
MCP_SERVER_URL=http://localhost:8000/mcp

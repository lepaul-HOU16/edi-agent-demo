# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# Cognito Authentication
COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
COGNITO_DOMAIN=your-app-domain

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
BEDROCK_REGION=us-east-1

# Storage
STORAGE_BUCKET_NAME=your-storage-bucket
S3_REGION=us-east-1

# API Configuration
API_GATEWAY_URL=https://xxxxxxxxxx.execute-api.us-east-1.amazonaws.com
API_STAGE=prod

# DynamoDB Tables
CHAT_MESSAGE_TABLE=ChatMessage-prod
CHAT_SESSION_TABLE=ChatSession-prod
SESSION_CONTEXT_TABLE=SessionContext-prod
AGENT_PROGRESS_TABLE=AgentProgress-prod

# Lambda Configuration
LAMBDA_TIMEOUT=300
LAMBDA_MEMORY=1024
LAMBDA_RUNTIME=nodejs20.x
PYTHON_LAMBDA_RUNTIME=python3.12

# Logging and Monitoring
LOG_LEVEL=INFO
ENABLE_XRAY=false
CLOUDWATCH_LOG_RETENTION_DAYS=7

# Feature Flags
ENABLE_ASYNC_PROCESSING=true
ENABLE_THOUGHT_STEPS=true
ENABLE_ARTIFACT_GENERATION=true

# External APIs (Optional)
WEATHER_API_KEY=your-weather-api-key
NREL_API_KEY=your-nrel-api-key

# Development Settings
NODE_ENV=development
DEBUG=false

# CDK Deployment
CDK_DEFAULT_ACCOUNT=123456789012
CDK_DEFAULT_REGION=us-east-1
STACK_NAME=AgentPlatformStack
ENVIRONMENT=dev

# Cost Optimization
ENABLE_LAMBDA_INSIGHTS=false
ENABLE_DYNAMODB_AUTOSCALING=true
S3_LIFECYCLE_DAYS=90

# Security
ENABLE_WAF=false
ALLOWED_ORIGINS=http://localhost:3000,https://your-domain.com
JWT_EXPIRATION_HOURS=1

# Monitoring Alerts
ALERT_EMAIL=alerts@your-org.com
ALERT_SNS_TOPIC_ARN=arn:aws:sns:us-east-1:123456789012:alerts

# Performance
MAX_CONCURRENT_EXECUTIONS=50
RESERVED_CONCURRENT_EXECUTIONS=10
PROVISIONED_CONCURRENCY=0

# Custom Agent Configuration
ENABLE_WEATHER_AGENT=true
ENABLE_CALCULATOR_AGENT=true
ENABLE_DATA_ANALYSIS_AGENT=true

{
  "name": "aws-energy-insights-starter-kit",
  "version": "1.0.0",
  "description": "Starter kit for building AI-powered agent platforms using AWS Bedrock, Lambda, and DynamoDB",
  "main": "index.js",
  "scripts": {
    "build": "tsc && cd cdk && npm run build",
    "test": "jest",
    "test:integration": "node scripts/test-integration.js",
    "test:coverage": "jest --coverage",
    "test:load": "artillery run load-test.yml",
    "cdk": "cd cdk && cdk",
    "deploy": "cd cdk && cdk deploy --all",
    "deploy:dev": "cd cdk && cdk deploy --all --context environment=dev",
    "deploy:prod": "cd cdk && cdk deploy --all --context environment=prod --require-approval never",
    "destroy": "cd cdk && cdk destroy --all",
    "synth": "cd cdk && cdk synth",
    "diff": "cd cdk && cdk diff",
    "bootstrap": "cd cdk && cdk bootstrap",
    "logs:dev": "aws logs tail /aws/lambda/dev-agent-platform-chat --follow",
    "logs:prod": "aws logs tail /aws/lambda/prod-agent-platform-chat --follow",
    "monitor:dev": "node scripts/monitor.js --environment=dev",
    "monitor:prod": "node scripts/monitor.js --environment=prod",
    "create-user": "node scripts/create-user.js",
    "test-agent": "node scripts/test-agent.js",
    "estimate-cost": "node scripts/estimate-cost.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "prepare": "husky install"
  },
  "keywords": [
    "aws",
    "bedrock",
    "lambda",
    "ai",
    "agents",
    "chatbot",
    "starter-kit"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.490.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.490.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-lambda": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/lib-dynamodb": "^3.490.0",
    "aws-jwt-verify": "^4.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.133",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "artillery": "^2.0.3",
    "aws-cdk": "^2.118.0",
    "aws-cdk-lib": "^2.118.0",
    "constructs": "^10.3.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/aws-energy-insights-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/aws-energy-insights-starter-kit/issues"
  },
  "homepage": "https://github.com/your-org/aws-energy-insights-starter-kit#readme"
}

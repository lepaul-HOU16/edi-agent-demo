{
  "frontend": {
    "framework": "Next.js - SSG",
    "nodeVersion": "20",
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "cache": {
      "paths": [
        ".next/cache/**/*"
      ]
    }
  },
  "hosting": {
    "headers": [
      {
        "source": "/_next/static/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }
}
